<%- include('partials/header') %>
	<div class="container">
		<div class="thumbnail">
	        <img class="img-responsive" src="<%= employee.image %>">
	        <div class="caption-full">
	            <h4><a>Name: <%= employee['userId'].username %></a></h4>
	            <h4><a>Company: <%= employee.company %></a></h4>
	            <h4><a>Role: <%= employee.role %></a></h4>
	        </div>
	    </div>
	</div>
	<div class="container">
		<form method='get' action="/search">
            <div class="form-group">
                <input class="form-control" type="text" name="query" placeholder="Company">
            </div>
            <div class="form-group">
                <input class="btn btn-lg btn-primary btn-block" type="submit" placeholder="Search">
            </div>
        </form>
	</div>

<%- include('partials/footer') %>